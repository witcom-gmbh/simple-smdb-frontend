{
  "$schema": "./node_modules/ng-swagger-gen/ng-swagger-gen-schema.json",
  "swagger": "./swagger/smdb.json",
  "output": "./src/app/api",
  "includeTags": [
  "ServiceInstantiation_V2",  
  "ServiceRetrieval_V2",
  "ServiceMgmt_V1"
  ],
  "ignoreUnusedModels": true,
  "minParamsForContainer": 2,
  "sortParams": "desc",
  "defaultTag": "Api",
  "removeStaleFiles": true,
  "modelIndex": true,
  "serviceIndex": true,
  "apiModule": true,
  "enumModule": true,
  "generateExamples": false
}